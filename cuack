void keyPressed() {
  
//Para pantalla.
  if (keyPressed && pan == 0) {
      if (key == 'z' || key == 'Z') {
      camp = 1;
      XC=235;
      }
      }
  if(keyPressed && pan == 1){
     if (key == 'z' && pos == 0){
        camp = 2;
        }
     if (key == 'z' && pos == 1){
        camp = 12;
        }
     if (key == 'z' && pos == 2){
        camp = 22;
        }
        }
        
    if(keyPressed && pan == 3){
     if (key == 'z' && pos == 0){
        camp = 4;
        }
     if (key == 'z' && pos == 1){
        camp = 14;
        }
