        camp = 0;
        }
        }
  

//Para corazon en pantalla 1.
    if (key == CODED && pan == 1) {
       if (keyCode == LEFT){
       pos--;
       }
       if (keyCode == RIGHT){
       pos++;       
       }
    }
    if (key == CODED && pan == 3) {
       if (keyCode == LEFT){
